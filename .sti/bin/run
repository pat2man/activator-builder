
#!/bin/bash -e
#
# STI run script for the 'activator-builder' image.
# The run script execute the server that runs your application.
#
# For more informations see the documentation:
#	https://github.com/openshift/source-to-image/blob/master/docs/builder_image.md
#

ACTIVATOR_BIN_PATH="/opt/activator-${ACTIVATOR_EDITION}/activator"

exec ${ACTIVATOR_BIN_PATH} start
