{
    "apiVersion": "v1beta1",
    "kind": "BuildConfig",
    "labels": {
        "name": "sbt-0.13.5-build"
    },
    "metadata": {
        "name": "sbt-0.13.5-build"
    },
    "output": {
        "tag": "latest",
        "to": "ticketfly/sbt-0.13.5-repository"
    },
    "parameters": {
        "source": {
            "contextDir": ".",
            "git": {
                "ref": "master",
                "uri": "https://github.com/pat2man/sbt-builder.git"
            },
            "type": "Git"
        },
        "strategy": {
            "customStrategy": {
                "env": [
                    {
                        "name": "IMAGE_NAME",
                        "value": "ticketfly/sbt-0.13.5-builder"
                    },
                    {
                        "name": "SBT_VERSION",
                        "value": "0.13.5"
                    }
                ],
                "exposeDockerSocket": true,
                "image": "openshift/sti-image-builder"
            },
            "type": "Custom"
        }
    },
    "triggers": [
        {
            "github": {
                "secret": "2JgE7hh5qGpx_gc4f6FT"
            },
            "type": "github"
        },
        {
            "generic": {
                "secret": "VoEmks0jS3gNK2d8STPe"
            },
            "type": "generic"
        }
    ]
