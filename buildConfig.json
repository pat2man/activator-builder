{
    "apiVersion": "v1beta1",
    "kind": "BuildConfig",
    "labels": {
        "name": "activator-builder"
    },
    "metadata": {
        "name": "activator-builder"
    },
    "output": {
        "tag": "latest",
        "to": "activator-builder"
    },
    "parameters": {
        "source": {
            "contextDir": ".",
            "git": {
                "ref": "master",
                "uri": "https://github.com/pat2man/activator-builder.git"
            },
            "type": "Git"
        },
        "strategy": {
          "type": "Custom",
          "customStrategy": {
            "image": "openshift/sti-image-builder",
            "exposeDockerSocket": true
          }
        }
    },
    "triggers": [
        {
            "github": {
                "secret": "2JgE7hh5qGpx_gc4f6FT"
            },
            "type": "github"
        },
        {
            "generic": {
                "secret": "VoEmks0jS3gNK2d8STPe"
            },
            "type": "generic"
        }
    ]
}
